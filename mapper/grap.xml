<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="grap">
    <select id="select">
        select
            location
        from calendar
        where calendarId = 3
        order by location
    </select>
</mapper>
